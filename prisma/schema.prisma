generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(uuid())
  firstName   String
  lastName    String
  username    String   @unique
  email       String   @unique
  phoneNumber String   @unique
  password    String
  createdAt   DateTime @default(now())
}
